{
  "name": "hogwarts-library",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build -c=production",
    "postbuild:prod": "cd dist/assets/ && rm config*.json && mv config.build config.json",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage --silent",
    "test:changed": "ng test --onlyChanged",
    "test:watch": "jest --watch",
    "jest:clear": "jest --clearCache",
    "test:ci": "jest --coverage --reporters=jest-junit --reporters=default",
    "postinstall": "ngcc",
    "lint": "ng lint",
    "syncscript": "node sync.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@ctrl/ngx-github-buttons": "8.0.0",
    "angular-svg-icon": "14.0.0",
    "rxjs": "7.5.5",
    "tslib": "2.4.0",
    "whatwg-fetch": "3.6.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.2",
    "@angular/cli": "~14.0.2",
    "@angular/compiler-cli": "^14.0.0",
    "@ngneat/spectator": "^11.1.0",
    "@types/jest": "^28.1.3",
    "jest": "^28.1.1",
    "jest-junit": "^14.0.0",
    "jest-location-mock": "^1.0.9",
    "jest-preset-angular": "^12.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "typescript": "~4.7.2"
  }
}